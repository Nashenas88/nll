struct Map { value: Value }
struct Value { }

// fn lives_too_long<'s: 'r, 'r>(map: &'r Map) -> &'s Value {
//   &map.value
// }

for<'s: 'r, 'r>;
let map: &'r Map;
let return: &'s Value;

let m: &'_ Map;
let v: &'_ Value;

block START {
    map = use();
    m = &'_ *map;
    v = &'_ (*m).value;
    return = v;
}
