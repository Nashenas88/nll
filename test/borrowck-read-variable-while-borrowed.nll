// Corresponds to:
//
// ```
// let foo = Vec { dummy: () };
// let p = &mut foo;
// use(foo);
// use(p);
// ```
//
// Should be error, since `q` forces borrow of `p` to outlive `foo`.

struct Vec<+> {
  field: 0
}  

let foo: Vec<()>;
let p: &'p mut Vec<()>;

block START {
    foo = use();
    p = &'b1 mut foo;
    use(foo); //! ERROR
    use(p);
    StorageDead(p);
    StorageDead(foo);
}

