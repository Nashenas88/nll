// Corresponds to:
//
// ```
// let a = Foo { ... };
// let b = &mut *a.field;
// a = Foo { .. };
// use(b);
// ```
//
// Should be OK; `b` will not be invalidated by
// overwriting `a`, and no new alias is created.

struct Foo<'-> {
  field: &'0 mut ()
}  

let a: Foo<'_>;
let b: &'_ mut ();

block START {
    a = use();
    b = &'_ mut a.field.star;
    a = use();
    use(b);
    StorageDead(b);
    StorageDead(a);
}

