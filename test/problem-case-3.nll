block START {
    * $GET1;
    goto SOME NONE;
}

//    Some(value) => value,
block SOME {
    * $GET1;
    {START -> m/0 } <= $GET1;
    goto EXIT;
}

//    None => { map.insert(..); map.get(&key) }
block NONE {
    * $GET2;
    {START -> m/0 } <= $GET2;
    goto EXIT;
}

//   }
