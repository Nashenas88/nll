block EXIT {
    goto m;
}

block m {
}

assert $GET1 = {START -> SOME/1, m/0};
assert SOME/0 in $GET1;
assert NONE/0 not in $GET1;
assert m/0 in $GET1;

assert NONE/0 in $GET2;
assert m/0 in $GET2;
