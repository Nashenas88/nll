block START {
    * $R ;
    goto CHECK;
}

block CHECK {
    goto FIND ;
}

block FIND {
    * $R ;
    * $Q ;
    goto OK ERR;
}

block OK {
    * $R ;
    * $Q ;
    $Q <= $U;
    goto EXIT;
}

block ERR {
    goto CHECK;
}    

block EXIT {
    * $R ;
    * $Q ;
}

assert $R = {START -> EXIT/0, ERR/0};
assert $Q = {FIND -> EXIT/1};
assert $U = {OK -> EXIT/1};
assert START/1 in $R;
assert FIND/0 in $R;
assert FIND/0 in $Q;
assert ERR/0 in $R;
assert ERR/0 not in $Q;
assert ERR/0 not in $U;