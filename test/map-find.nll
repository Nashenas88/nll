block START {
    * $R ;
    goto FIND;
}

block CHECK {
    goto FIND ;
}

block FIND {
    * $R ;
    * $Q ;
    goto OK ERR;
}

block OK {
    * $R ;
    * $Q ;
    $Q <= $U;
    goto EXIT;
}

block ERR {
    goto CHECK;
}    

block EXIT {
    * $R ;
    * $Q ;
}

assert $R = {EXIT/1};
assert $Q = {EXIT/2, ERR/0};
assert $U = {EXIT/2};
assert START/1 in $R;
assert FIND/0 in $R;
assert FIND/0 in $Q;