let p: Ref<', Foo>;

block START {
    p = &'A;
    use(p);
    goto A;
}

block A {
    goto B C EXIT;
}

block B {
    use(p);
    ;
    p = &'B;
    goto C;
}

block C {
    use(p);
    goto A;
}

block EXIT {
}

'A = {A/[0..1], B/[0..1], C/[0..2], START/[0..3]}
'B = {A/[0..1], B/[0..4], C/[0..2]}
